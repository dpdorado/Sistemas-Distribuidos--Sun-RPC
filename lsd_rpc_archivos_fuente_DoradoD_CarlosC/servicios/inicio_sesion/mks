
# This is a template Makefile generated by rpcgen

# Parameters

CLIENT = 
SERVER = servidor_is

SOURCES_CLNT.c = 
SOURCES_CLNT.h = 
SOURCES_SVC.c = servidor_is.c
SOURCES_SVC.h = servicio_inicio_sesion.h ./librerias/std_is.h
SOURCES.x = servicio_inicio_sesion.x

TARGETS_SVC.c = servicio_inicio_sesion_svc.c   servicio_inicio_sesion_xdr.c	./librerias/std_is.c
TARGETS_CLNT.c =     servicio_inicio_sesion_xdr.c 
TARGETS = servicio_inicio_sesion.h servicio_inicio_sesion_xdr.c   servicio_inicio_sesion_svc.c	./librerias/std_is.h ./librerias/std_is.c

OBJECTS_CLNT = $(SOURCES_CLNT.c:%.c=%.o) $(TARGETS_CLNT.c:%.c=%.o)
OBJECTS_SVC = $(SOURCES_SVC.c:%.c=%.o) $(TARGETS_SVC.c:%.c=%.o)
# Compiler flags 

CFLAGS += -g 
LDLIBS += -lnsl
RPCGENFLAGS = 

# Targets 

all : $(CLIENT) $(SERVER)

$(TARGETS) : $(SOURCES.x) 
	rpcgen $(RPCGENFLAGS) $(SOURCES.x)

$(OBJECTS_CLNT) : $(SOURCES_CLNT.c) $(SOURCES_CLNT.h) $(TARGETS_CLNT.c) 

$(OBJECTS_SVC) : $(SOURCES_SVC.c) $(SOURCES_SVC.h) $(TARGETS_SVC.c) 

$(CLIENT) : $(OBJECTS_CLNT) 
	$(LINK.c) -o $(CLIENT) $(OBJECTS_CLNT) $(LDLIBS) 

$(SERVER) : $(OBJECTS_SVC) 
	$(LINK.c) -o $(SERVER) $(OBJECTS_SVC) $(LDLIBS)

 clean:
	 $(RM) core $(TARGETS) $(OBJECTS_CLNT) $(OBJECTS_SVC) $(CLIENT) $(SERVER)

